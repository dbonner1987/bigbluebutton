 \"bigbluebutton:to-bbb-apps:whiteboard\";\n\nconfig.redis.channels.toBBBApps.polling = \"bigbluebutton:to-bbb-apps:polling\";\n\nconfig.log = {};\n\nif (Meteor.isServer) {\n  config.log.path = (typeof process !== \"undefined\" && process !== null ? (ref = process.env) != null ? ref.NODE_ENV : void 0 : void 0) === \"production\" ? \"/var/log/bigbluebutton/bbbnode.log\" : process.env.PWD + '/../log/development.log';\n  winston = Winston;\n  file = config.log.path;\n  transports = [\n    new winston.transports.Console(), new winston.transports.File({\n      filename: file\n    })\n  ];\n  Meteor.log = new winston.Logger({\n    transports: transports\n  });\n}\n\nMeteor.config = config;\n","sourceMap":{"version":3,"file":"/config.coffee.js","sourceRoot":"","sources":["/config.coffee"],"names":[],"mappings":";AAGA,IAAA,sCAAA;;AAAA,MAAA,GAAS,EAAT,CAAA;;AAAA,MAGM,CAAC,OAAP,GAAiB,4BAHjB,CAAA;;AAAA,MAIM,CAAC,gBAAP,GAA0B,UAJ1B,CAAA;;AAAA,MAKM,CAAC,aAAP,GAAuB,MALvB,CAAA;;AAAA,MAMM,CAAC,gBAAP,GAA0B,MAN1B,CAAA;;AAAA,MAOM,CAAC,qBAAP,GAA+B,qTAP/B,CAAA;;AAAA,MAQM,CAAC,2BAAP,GAAqC,iIAAA,GAAkI,MAAM,CAAC,gBAAzI,GAA0J,WAR/L,CAAA;;AAAA,MAUM,CAAC,iBAAP,GAA2B,EAV3B,CAAA;;AAAA,MAWM,CAAC,aAAP,GAAuB,GAXvB,CAAA;;AAAA,MAaM,CAAC,UAAP,GAAoB,IAbpB,CAAA;;AAAA,MAgBM,CAAC,GAAP,GAAa,EAhBb,CAAA;;AAAA,MAmBM,CAAC,GAAG,CAAC,UAAX,GAAsB,EAnBtB,CAAA;;AAAA,MAoBM,CAAC,GAAG,CAAC,WAAX,GAAuB,EApBvB,CAAA;;AAAA,MAuBM,CAAC,GAAG,CAAC,aAAX,GAA2B,KAvB3B,CAAA;;AAAA,MAyBM,CAAC,GAAG,CAAC,yBAAX,GAAuC,IAzBvC,CAAA;;AAAA,MA4BM,CAAC,KAAP,GAAe,EA5Bf,CAAA;;AAAA,MA6BM,CAAC,KAAK,CAAC,IAAb,GAAoB,WA7BpB,CAAA;;AAAA,MA8BM,CAAC,KAAK,CAAC,IAAb,GAAoB,MA9BpB,CAAA;;AAAA,MA+BM,CAAC,KAAK,CAAC,OAAb,GAAuB,IA/BvB,CAAA;;AAAA,MAgCM,CAAC,KAAK,CAAC,QAAb,GAAwB,EAhCxB,CAAA;;AAAA,MAiCM,CAAC,KAAK,CAAC,QAAQ,CAAC,WAAtB,GAAoC,+BAjCpC,CAAA;;AAAA,MAkCM,CAAC,KAAK,CAAC,QAAQ,CAAC,SAAtB,GAAkC,EAlClC,CAAA;;AAAA,MAmCM,CAAC,KAAK,CAAC,QAAQ,CAAC,SAAS,CAAC,OAAhC,GAA0C,6BAnC1C,CAAA;;AAAA,MAoCM,CAAC,KAAK,CAAC,QAAQ,CAAC,SAAS,CAAC,IAAhC,GAAuC,gCApCvC,CAAA;;AAAA,MAqCM,C