{"source":"__coffeescriptShare = typeof __coffeescriptShare === 'object' ? __coffeescriptShare : {}; var share = __coffeescriptShare;\nthis.WhiteboardToolModel = (function() {\n  function WhiteboardToolModel() {}\n\n  WhiteboardToolModel.prototype.initialize = function(paper) {\n    this.paper = paper;\n    console.log(\"paper:\" + this.paper);\n    this.gh = 0;\n    this.gw = 0;\n    this.obj = 0;\n    return this.definition = [];\n  };\n\n  WhiteboardToolModel.prototype.setPaperSize = function(gh, gw) {\n    this.gh = gh;\n    this.gw = gw;\n  };\n\n  WhiteboardToolModel.prototype.setOffsets = function(xOffset, yOffset) {\n    this.xOffset = xOffset;\n    this.yOffset = yOffset;\n  };\n\n  WhiteboardToolModel.prototype.setPaperDimensions = function(paperWidth, paperHeight) {\n    this.paperWidth = paperWidth;\n    this.paperHeight = paperHeight;\n  };\n\n  WhiteboardToolModel.prototype.getDefinition = function() {\n    return this.definition;\n  };\n\n  WhiteboardToolModel.prototype.hide = function() {\n    if (this.obj != null) {\n      return this.obj.hide();\n    }\n  };\n\n  return WhiteboardToolModel;\n\n})();\n","sourceMap":{"version":3,"file":"/client/whiteboard_models/_whiteboard_tool.coffee.js","sourceRoot":"","sources":["/client/whiteboard_models/_whiteboard_tool.coffee"],"names":[],"mappings":";AACA,IAAO,CAAA;mCAEL;;AAAA,gCAAA,UAAA,GAAY,SAAC,KAAD,GAAA;AACV,IADW,IAAC,CAAA,QAAD,KACX,CAAA;AAAA,IAAA,OAAO,CAAC,GAAR,CAAY,QAAA,GAAW,IAAC,CAAA,KAAxB,CAAA,CAAA;AAAA,IACA,IAAC,CAAA,EAAD,GAAM,CADN,CAAA;AAAA,IAEA,IAAC,CAAA,EAAD,GAAM,CAFN,CAAA;AAAA,IAGA,IAAC,CAAA,GAAD,GAAO,CAHP,CAAA;WAKA,IAAC,CAAA,UAAD,GAAc,GANJ;EAAA,CAAZ,CAAA;;AAAA,gCAWA,YAAA,GAAc,SAAC,EAAD,EAAM,EAAN,GAAA;AAAY,IAAX,IAAC,CAAA,KAAD,EAAW,CAAA;AAAA,IAAN,IAAC,CAAA,KAAD,EAAM,CAAZ;EAAA,CAXd,CAAA;;AAAA,gCAaA,UAAA,GAAY,SAAC,OAAD,EAAW,OAAX,GAAA;AAAsB,IAArB,IAAC,CAAA,UAAD,OAAqB,CAAA;AAAA,IAAX,IAAC,CAAA,UAAD,OAAW,CAAtB;EAAA,CAbZ,CAAA;;AAAA,gCAeA,kBAAA,GAAoB,SAAC,UAAD,EAAc,WAAd,GAAA;AAA6B,IAA5B,IAAC,CAAA,aAAD,UAA